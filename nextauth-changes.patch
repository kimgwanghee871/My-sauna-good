@@ -0,0 +1,67 @@
+#!/bin/bash
+
+# G-Won AI Planner Auto-Save System for Deployment
+# Protects against sandbox 1-hour deletion problem
+
+PROJECT_DIR="/home/user/webapp/My-sauna-deploy"
+LOG_FILE="/home/user/webapp/auto-save-deploy.log"
+COMMIT_INTERVAL=120  # 2 minutes
+
+# Function to log with timestamp
+log_message() {
+    echo "[$(date '+%Y-%m-%d %H:%M:%S')] $1" | tee -a "$LOG_FILE"
+}
+
+# Function to auto-commit changes
+auto_commit() {
+    cd "$PROJECT_DIR" || {
+        log_message "❌ ERROR: Cannot access project directory $PROJECT_DIR"
+        return 1
+    }
+    
+    # Check for changes
+    if [[ -n $(git status --porcelain) ]]; then
+        log_message "📝 Changes detected, committing..."
+        
+        git add .
+        
+        COMMIT_MSG="🔄 Auto-save: $(date '+%Y-%m-%d %H:%M:%S') - Sandbox protection"
+        
+        if git commit -m "$COMMIT_MSG"; then
+            log_message "✅ Changes committed successfully"
+            
+            # Try to push to GitHub
+            if git push origin main; then
+                log_message "🚀 Changes pushed to GitHub successfully"
+            else
+                log_message "⚠️  Push failed, will retry next cycle"
+            fi
+        else
+            log_message "⚠️  Commit failed"
+        fi
+    else
+        log_message "✨ No changes to commit"
+    fi
+}
+
+# Main execution
+log_message "🚀 Starting G-Won AI Planner Auto-Save System (Deploy Version)"
+log_message "📁 Project directory: $PROJECT_DIR"
+log_message "⏰ Commit interval: $COMMIT_INTERVAL seconds"
+
+# Verify project directory exists
+if [[ ! -d "$PROJECT_DIR" ]]; then
+    log_message "❌ ERROR: Project directory does not exist: $PROJECT_DIR"
+    exit 1
+fi
+
+# Initial status check
+cd "$PROJECT_DIR" && git status
+log_message "📊 Git repository status verified"
+
+# Main loop
+while true; do
+    auto_commit
+    log_message "⏳ Waiting $COMMIT_INTERVAL seconds for next check..."
+    sleep $COMMIT_INTERVAL
+done
